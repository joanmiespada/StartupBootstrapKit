{

    "parser": "babel-eslint",
    "plugins": ["babel", "react"],
    "env":{
        "browser": true,
        "es6":true,
        "node":true
    },
    "extends": "react",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
          }
    },
    "rules":{
        "no-console":"warn",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}